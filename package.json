{
  "name": "vscode-kbank-ib20",
  "displayName": "vscode-kbank-ib20",
  "description": "ib20 sql dbio soultion",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-kbank-ib20.helloWorld",
    "onNotebook:my-notebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-kbank-ib20.helloWorld",
        "title": "IB20 DBIO"
      },
      {
        "command": "sqlmapExplorer.addEntry",
        "title": "Add Namespace",
        "icon": {
          "light": "resources/light/plus.svg",
          "dark": "resources/dark/plus.svg"
        }
      },
      {
        "command": "sqlmapExplorer.delEntry",
        "title": "Delete Namespace",
        "icon": {
          "light": "resources/light/minus.svg",
          "dark": "resources/dark/minus.svg"
        }
      },
      {
        "command": "sqlmapExplorer.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "Package Explorer",
          "icon": "resources/media/dep.svg"
        }
      ]
    },
    "views": {
      "package-explorer": [
        {
          "type": "webview",
          "id": "sqlconfigExplorer",
          "name": "Sql Config Explorer"
        },
        {
          "type": "tree",
          "id": "sqlmapExplorer",
          "name": "Sql Namespace Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sqlmapExplorer.addEntry",
          "when": "view == sqlmapExplorer",
          "group": "navigation"
        },
        {
          "command": "sqlmapExplorer.delEntry",
          "when": "view == sqlmapExplorer",
          "group": "navigation"
        },
        {
          "command": "sqlmapExplorer.refreshEntry",
          "when": "view == sqlmapExplorer",
          "group": "navigation"
        }
      ]
      
    },
    "notebooks": [
      {
          "id": "my-notebook",
          "type": "my-notebook",
          "displayName": "My Notebook",
          "selector": [
              {
                  "filenamePattern": "*.sql"
              }
          ]
      }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "esbuild": "^0.16.10",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/oracledb": "^5.2.4",
    "@vscode/webview-ui-toolkit": "^1.2.1",
    "ftp": "^0.3.10",
    "jsonc-parser": "^0.4.2",
    "minimist": "^1.2.6",
    "mkdirp": "^0.5.1",
    "monaco-editor": "^0.36.1",
    "mysql2": "^3.1.2",
    "nodejs-base64": "^2.0.0",
    "oracledb": "^5.5.0",
    "rimraf": "^2.6.2"
  }
}
